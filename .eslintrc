{
 "extends": "airbnb",
 "parserOptions": {
   "ecmaFeatures": {
     "jsx": true
     }
  },
  "env": {
    "browser": true,
    "node": true,
  },
 "parser": "babel-eslint",
 "plugins": [
   "react"
 ],
 "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "func-names": "off",
    "class-methods-use-this": "off",


    # "prefer-template": "off",
    # "func-call-spacing": "off",
    # "padded-block": "off",
    "react/jsx-filename-extension": [
      1, { "extensions": [".js", ".jsx"] }
    ],
   },
"globals": {

  }
}
